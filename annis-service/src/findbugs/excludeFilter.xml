<?xml version="1.0" encoding="UTF-8"?>

<!-- False positive Filter for FindBugs -->

<FindBugsFilter>
  <!-- automatically generated or external code that we can't influence -->
  <Match>
    <Or>
      <Class name="annis.ql.AqlLexer"/>
      <Class name="annis.ql.AqlParser"/>
      <Class name="annis.ql.AqlParser$IndirectPointingContext" />
      <Class name="annis.ql.AqlParser$RangePointingContext" />
      <Class name="annis.ql.AqlParser$AnnoEqTextExprContext" />
      <Class name="annis.ql.AqlParser$TokTextExprContext" />
      <Class name="annis.ql.AqlParser$MetaTermExprContext" />
      <Class name="annis.ql.RawAqlPreParser"/>
      <Class name="com.google.mimeparse.MIMEParse" />
      <Class name="com.google.mimeparse.MIMEParse$FitnessAndQuality" />
    </Or>
  </Match>
  <Match>
    <And>
      <Class name="annis.AnnisRunner" />
      <Bug pattern="DM_EXIT" />
    </And>
  </Match>
  <Match>
    <And>
      <Class name="annis.administration.AnnisAdminRunner" />
      <Bug pattern="DM_EXIT" />
    </And>
  </Match>
  
  <Match>
    <And>
      <Class name="annis.security.SerializableByteSource" />
      <Bug pattern="EI_EXPOSE_REP" />
    </And>
  </Match>
  <Match>
    <And>
      <Class name="annis.security.SerializableByteSource" />
      <Bug pattern="EI_EXPOSE_REP2" />
    </And>
  </Match>
  
  <Match>
    <Bug category="EXPERIMENTAL" />
  </Match>
  <Match>
    <Bug pattern="SE_NO_SERIALVERSIONID" />
  </Match>
  
</FindBugsFilter>
